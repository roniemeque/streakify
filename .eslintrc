{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "camelcase": 0,
    "react/react-in-jsx-scope": 0,
    "import/prefer-default-export": 0,
    "no-nested-ternary": 0,
    "max-len": 0
  }
}
